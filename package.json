{
  "license": "MIT",
  "version": "1.0.0",
  "name": "@qcz/raf-stages",
  "description": "Coordinate requestAnimationFrame calls",
  "homepage": "https://github.com/qcze/raf-stages#readme",
  "bugs": {
    "url": "https://github.com/qcze/raf-stages/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qcze/raf-stages.git"
  },
  "keywords": [
    "raf",
    "requestAnimationFrame"
  ],
  "files": [
    "build",
    "LICENSE",
    "README.md"
  ],
  "main": "./build/bundle/commonjs/index.cjs",
  "module": "./build/bundle/esmodule/index.mjs",
  "types": "./build/declaration/exports/index.d.ts",
  "typeVersions": {
    "*": {
      "index": [
        "./build/declaration/exports/index.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./build/bundle/esmodule/index.mjs",
      "require": "./build/bundle/commonjs/index.cjs",
      "default": "./build/bundle/commonjs/index.cjs"
    },
    "./index": {
      "import": "./build/bundle/esmodule/index.mjs",
      "require": "./build/bundle/commonjs/index.cjs",
      "default": "./build/bundle/commonjs/index.cjs"
    }
  },
  "scripts": {
    "build": "run-s build:clean build:declaration build:bundle",
    "build:clean": "rimraf build",
    "build:declaration": "ttsc -p scripts/typescript/tsconfig.declaration.json",
    "build:bundle": "rollup -c scripts/rollup/rollup.config.ts --configPlugin @rollup/plugin-typescript"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/babel__core": "^7.1.17",
    "@types/node": "^17.0.4",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-plugin-module-resolver": "^4.1.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-terser": "^7.0.2",
    "ttypescript": "^1.5.13",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.16.5"
  }
}
